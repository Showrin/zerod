{
  "name": "@zerod/zerod",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "An interactive, open-source component library for reactjs projects. Lets build web applications with no dimension or zero dimension. That's why we named it ZeroD(0D).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Showrin/zerod.git"
  },
  "author": "Showrin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Showrin/zerod/issues"
  },
  "homepage": "https://github.com/Showrin/zerod#readme",
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build-storybook": "build-storybook",
    "build": "lerna exec --parallel -- babel --root-mode upward src -d lib --ignore **/*.stories.jsx,**/*.stories.js,**/*.spec.jsx,**/*.spec.js",
    "coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^1.13.3",
    "jest": "^27.3.1",
    "jest-haste-map": "^27.3.1",
    "jest-resolve": "^27.3.1",
    "jest-styled-components": "^7.0.8",
    "lerna": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "styled-components": "^5.3.3",
    "webpack": "^5.64.0"
  },
  "dependencies": {}
}
